      <!--News Section-->
      <section id="about-1" class="news-section about">
        <div class="auto-container">
          <!--Sec Title-->
          <div class="sec-title centered">
              <h2>Join the Fight</h2>
          </div>
          <div class="row clearfix">
            <!--News Block-->
            <div class="news-block col-md-4 col-sm-6 col-xs-12">
              <div class="inner-box">
                <div class="text-center">
                  <%= image_tag "target.png" ,style:"border-radius: 10px", alt: "Discover Clans Image" %>
                </div>
              <div class="lower-box">
                <h3>Discover</h3>
                  <div class="text">Find the right community and members for you based on age, activity, location, and fifteen advanced filters. Have more fun with player preferences, ratings, and algorithms that help you join the right games and groups.</div>
                    </div>
              </div>
            </div>
            <!--End News Block-->

            <!--News Block-->
            <div class="news-block col-md-4 col-sm-6 col-xs-12">
              <div class="inner-box">
                <div class="text-center">
                  <%= image_tag "nav.png" ,style:"border-radius: 10px" , alt:"Organize Your Clan Image" %>
                </div>
                <div class="lower-box">
                  <h3>Organize</h3>
                  <div class="text">Interactive game rosters support an endless number of game titles and sync across Discord, Twitch, and social networks. Share on any forum, game site, or email to reach your team anywhere.  Everyone is always just one click away from joining events.</div>
                </div>
              </div>
            </div>
            <!--End News Block-->

            <!--News Block-->
            <div class="news-block col-md-4 col-sm-6 col-xs-12">
              <div class="inner-box">
                <div class="text-center">
                  <%= image_tag "tools.png" , style:"border-radius: 10px" , alt:"Build Your Clan Image" %>
                </div>
                <div class="lower-box">
                  <h3>Build</h3>
                  <div class="text">Whether your an eSports team, streamer, charity, large clan, college, or a small group of friends, we have everything you need with a clan LFG, Discord bots, Twitch commands, clan applications, donations, clan chat, customizable ranks, mobile apps, and more. </div>
                </div>
              </div>
            </div>
            <!--End News Block-->
          </div>
        </div>

        <div class="auto-container">
          <div class="row clearfix">
            <!--News Block-->
            <div class="news-block col-md-4 col-sm-6 col-xs-12">
              <div class="inner-box">
                <div class="text-center">
                  <%= image_tag "crest.png" , style:"border-radius: 10px" , alt:"Discover Clans Image" %>
                </div>
                <div class="lower-box">
                  <h3>Clans</h3>
                  <div class="text"> With hundreds of configuration options, you can quickly discover or build any type of community. Set up a gaming business coaching players, collect donations for your charity, promote your competitive team, streamline gaming night, or simply connect with fans. </div>
                </div>
              </div>
            </div>
                <!--End News Block-->
                <!--News Block-->
            <div class="news-block col-md-4 col-sm-6 col-xs-12">
              <div class="inner-box">
                <div class="text-center">
                  <%= image_tag "rooster.png" ,style:"border-radius: 10px", alt:"Organize Your Clan Image" %>
                </div>
                <div class="lower-box">
                  <h3>Players</h3>
                  <div class="text">Detailed profiles, activity tracking, ratings, and fifteen advanced filters help you minimize lag and find the right player for your games and communities. Avoid trolls with player preferences that automatically screen and block players.</div>
                </div>
              </div>
            </div>
                <!--End News Block-->
                <!--News Block-->
            <div class="news-block col-md-4 col-sm-6 col-xs-12">
